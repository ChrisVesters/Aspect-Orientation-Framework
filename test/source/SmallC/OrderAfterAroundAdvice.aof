order{b; c; a}

pointcut p() {
	execute int mySquare(int);
}

pointcut q(int i) {
	call int random(int i);
}

advice a: after p() {
	printf("BEGIN A\n");
}

advice b: after p() {
	printf("BEGIN B\n");
}

advice c: around p() {
	int retVal = 0;
	printf("BEGIN C!\n");
	retVal = proceed();
	printf("END C!\n");
	return retVal;
}
