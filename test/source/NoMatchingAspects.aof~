pointcut p() {
	execute int random(int);
}

pointcut q(int i) {
	call int random(int i);
}

advice a: before p() {
	printf("Advice A\n");
}

advice b: after p() {
	printf("Advice B\n");
}

advice c: around q(int i) {
	int retVal = 0;	
	printf("BEGIN C!\n");
	retVal = proceed(i);
	printf("END C!\n");
	return retVal;
}

before test() {
	printf("Advice ?\n");
}

after q(char[] a) {
	printf("String = %s\n", a);
}

before test(int j) {
	printf("i = %i\n", j);	
}

after test(int j) {
	printf("i was %i\n", j);
}
